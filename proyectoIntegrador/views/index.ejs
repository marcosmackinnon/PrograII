<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="/users/logo-mercado-liebre.jpg" />
	
	<title> Compra Mercedes Benz Argentina</title>
			<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/app.css">
</head>


  <body>
    <%- include('partials/pHeaderNoLogueado') %>

    <h2 class = "bienvenido">Bienvenido a "Compra Mercedes Benz Argentina"</h2>
    
    <!-- Banner -->
	<div class="banner">
		<img src="" alt="">
	</div>
	<!-- /Banner -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title"> Nuestros autos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="/product/1">
						<figure class="product-box_image">
							<img src="/images/autos/mercedesA200.avif" alt="A200">
						</figure>
						<article class="product-box_data">
							<h2>Mercedes A200</h2>
							<p>Auto deportivo</p>
							<p>Comentarios: 12</p>
						</article>
					</a>
				</section>
			</div>
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="/product/2">
						<figure class="product-box_image">
							<img src="images/autos/mercedesC200.jpeg" alt="C200">
						</figure>
						<article class="product-box_data">
							<h2>Mercedes C200</h2>
							<p>Auto lindo</p>
							<p>Comentarios: 12</p>
						</article>
					</a>
				</section>
			</div>
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="/product/3">
						<figure class="product-box_image">
							<img src="images/autos/mercedezAMGGT.jpeg" alt="Amg">
						</figure>
						<article class="product-box_data">
							<h2>Mercedes AMG</h2>
							<p>Nave</p>
							<p>Comentarios: 12</p>
						</article>
					</a>
				</section>
			</div>
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="/product/4">
						<figure class="product-box_image">
							<img src="images/autos/mercedezEclass.webp" alt="Eclass">
						</figure>
						<article class="product-box_data">
							<h2>Mercedes Eclass</h2>
							<p>Mecha</p>
							<p>Comentarios: 12</p>
						</article>
					</a>
				</section>
			</div>
		</div>
	</div>



    <%- include('partials/footer') %>
  </body>

</html>
