<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="./images/logo-mercado-liebre.jpg" />

	<title> Mercado Liebre Argentina</title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="/PrograII/proyectoIntegrador/public/stylesheets/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="/PrograII/proyectoIntegrador/public/stylesheets/app.css">
</head>

<body>
	<!-- Header -->
	<%- include('partials/pHeaderNoLogueado') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Ingresar</h2>
			</div>
			<!-- Show the "ultimos" products -->

			<!--FORMULARIO DE LOGIN -->
			<form action="/users/login" method="GET">
				<div>
				  <label for="email">Email:</label>
				  <input type="email" name="email" id="email" required>
				</div>
			  
				<div>
				  <label for="password">Contraseña:</label>
				  <input type="password" name="password" id="password" required>
				</div>
			  
				<div>
				  <label for="recordarme">
					<input type="checkbox" name="recordarme" id="recordarme">
					Recordarme
				  </label>
				</div>
			  
				<button type="submit">Iniciar sesión</button>
			  </form>
			  


	<!-- footer -->
	<%- include('partials/footer') %>
	<!-- /footer -->
</body>

</html>